@model IEnumerable<HomeManagement.Domain.Pumping>

@{
    ViewBag.Title = "Index";
}

<h2>All Pumpings</h2>

<table>
    <tr>
        <th>
            StartTime
        </th>
        <th>
            EndTime
        </th>
        <th>
            Milliliters
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PumpingTimes.FirstOrDefault().StartTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PumpingTimes.LastOrDefault().EndTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Milliliters)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

<p>
    @Html.ActionLink("New", "Create")
</p>